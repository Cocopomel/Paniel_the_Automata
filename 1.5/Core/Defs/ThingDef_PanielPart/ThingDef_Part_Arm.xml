<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  <ThingDef Name="PN_PartItemBase_Arm" ParentName="PN_PartItemBase" Abstract="True">
    <stackLimit>1</stackLimit>
    <statBases>
      <Mass>1</Mass>
    </statBases>
    <thingCategories>
      <li>PN_PartC_Arm</li>
    </thingCategories>
    <comps>
      <li>
        <compClass>CompQuality</compClass>
      </li>
    </comps>
  </ThingDef>

  <!--급조(기본) 팔-->

  <ThingDef ParentName="PN_PartItemBase_Arm">
    <defName>PN_ATModule_BaseArm</defName>
    <label>paniel makeshift arm</label>
    <description>.</description>
    <graphicData Inherit="false">
      <graphicClass>Graphic_Single</graphicClass>
      <texPath>Things/Item/PNModuleIndustrial</texPath>
    </graphicData>

    <modExtensions>
      <li Class="ModuleAutomata.AutomataModuleExtension">
        <isDefault>true</isDefault>
        <targetParts>
          <li>LeftArm</li>
          <li>RightArm</li>
        </targetParts>
        <hediff>PN_BaseArm</hediff>
      </li>
    </modExtensions>
  </ThingDef>

  <HediffDef ParentName="AddedBodyPartBase">
    <defName>PN_BaseArm</defName>
    <label>makeshift arm</label>
    <description>.</description>
    <addedPartProps>
      <solid>true</solid>
      <partEfficiency>0.6</partEfficiency>
    </addedPartProps>
  </HediffDef>

  <!--수제-->

  <ThingDef ParentName="PN_PartItemBase_Arm">
    <defName>PN_ATModule_HandiArm</defName>
    <label>paniel handicraft arm</label>
