<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  <ThingDef Name="PN_PartItemBase_Body" ParentName="PN_PartItemBase" Abstract="True">
    <stackLimit>1</stackLimit>
    <statBases>
      <Mass>1</Mass>
    </statBases>
    <thingCategories>
      <li>PN_PartC_Body</li>
    </thingCategories>
    <comps>
