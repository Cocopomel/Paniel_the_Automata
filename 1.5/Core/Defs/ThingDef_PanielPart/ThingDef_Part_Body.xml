        <compClass>CompQuality</compClass>
      </li>
    </comps>
  </ThingDef>

  <!--급조-->

  <ThingDef ParentName="PN_PartItemBase_Body">
    <defName>PN_ATModule_BaseChassi</defName>
    <label>paniel makeshift Chassi</label>
    <description>.</description>
    <graphicData Inherit="false">
      <graphicClass>Graphic_Single</graphicClass>
      <texPath>Things/Item/PNModuleIndustrial</texPath>
    </graphicData>

    <modExtensions>
      <li Class="ModuleAutomata.AutomataModuleExtension">
        <isDefault>true</isDefault>
        <targetParts>
          <li>MainFrame</li>
        </targetParts>
        <hediff>PN_BaseChassi</hediff>
      </li>
    </modExtensions>
  </ThingDef>

  <HediffDef>
    <defName>PN_BaseChassi</defName>
    <label>makeshift Chassi</label>
    <description>.</description>

    <hediffClass>HediffWithComps</hediffClass>
    <defaultLabelColor>(0.6, 0.6, 1.0)</defaultLabelColor>
    <stages>
      <li>
        <capMods>
          <li>
            <capacity>Consciousness</capacity>
            <postFactor>0.80</postFactor>
          </li>
          <li>
            <capacity>Sight</capacity>
            <postFactor>0.60</postFactor>
          </li>
          <li>
            <capacity>Hearing</capacity>
            <postFactor>0.60</postFactor>
          </li>
        </capMods>
        <statOffsets>
          <ArmorRating_Sharp>0.1</ArmorRating_Sharp>
          <ArmorRating_Blunt>0.1</ArmorRating_Blunt>
          <ArmorRating_Heat>0.1</ArmorRating_Heat>
        </statOffsets>
      </li>
    </stages>

    <isBad>false</isBad>
    <priceImpact>true</priceImpact>
    <allowMothballIfLowPriorityWorldPawn>true</allowMothballIfLowPriorityWorldPawn>
  </HediffDef>

